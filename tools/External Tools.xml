      <option name="COMMAND" value="/opt/local/payara.current/bin/asadmin" />
      <option name="PARAMETERS" value="set configs.config.server-config.cdi-service.enable-implicit-cdi=false" />
      <option name="WORKING_DIRECTORY" value="/opt/local/payara.current/bin" />
    </exec>
  </tool>
  <tool name="Thunar" showInMainMenu="true" showInEditor="true" showInProject="true" showInSearchPopup="false" disabled="false" useConsole="false" showConsoleOnStdOut="false" showConsoleOnStdErr="false" synchronizeAfterRun="false">
